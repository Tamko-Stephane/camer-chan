<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - CHAN 2021 Cameroun</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>    
    @Html.Partial("_navbar")

<div class="container">
    <div class="chan-container">
        <div class="row">        
            @*<img src="~/Content/Images/cameroon_chan_2021_FTS_1101.png" alt="CHAN Cameroun 2021" class="w-100"/>*@
            @RenderBody()
            <p class="blockquote-footer">
                <span>&copy; @DateTime.Now.Year - CHAN 2021</span>
                <br/>
                <code><a href="https://twitter.com/FlywingsS" target="_blank">@@Flywings</a></code>
            </p>
        </div>
        @*<hr/>*@
    </div>
</div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
